{
  "name": "cc-speed",
  "version": "0.1.2",
  "description": "Analyze Claude Code token output speed from local JSONL logs",
  "type": "module",
  "bin": {
    "cc-speed": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "files": [
    "dist",
    "install-skill.sh"
  ],
  "scripts": {
    "build": "tsup && chmod +x dist/index.js",
    "dev": "tsup --watch",
    "test": "vitest run",
    "postinstall": "node ./dist/postinstall.js",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "claude",
    "claude-code",
    "token-speed",
    "benchmark",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "glob": "^11.0.0",
    "ora": "^8.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
